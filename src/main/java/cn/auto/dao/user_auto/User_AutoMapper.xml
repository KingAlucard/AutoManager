<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.auto.dao.user_auto.User_AutoMapper">
    <update id="findUpdateAuto">
        UPDATE auto SET autoName=?,production=?,autoTypeId,autoMoney=?,autoImg=?,adminAuto=?,userAuto=?,autoConfig=? WHERE Id=?
        <trim prefix="set" suffix="," suffixOverrides="where">
            <if test="autoName != null and autoName != ''">
                autoName = #{autoName}
            </if>
        </trim>
    </update>
</mapper>